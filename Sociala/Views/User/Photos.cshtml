
@{
	ViewBag.Title = "Photos";
}
<div class="middle-sidebar-bottom">
    <div class="middle-sidebar-left pe-0">
        <div class="row">
            <div class="col-xl-12">


                <div class="row ps-2 pe-2">

                    @foreach (Post post in (List<Post>)ViewBag.Photos)
                    {
                        <div class="card-body d-flex pt-4 ps-4 pe-4 pb-0 border-top-xs bor-0">
                             <a asp-controller="User" asp-action="Profile" asp-route-Id="@post.UserId">
                                            <figure class="avatar ms-auto me-auto mb-0 position-relative w65 z-index-1">
                                                <img src="@post.Imj" alt="image" class="float-right p-0 bg-white rounded-circle w-100 shadow-xss">
                                            </figure>
                                        </a>
                          
                        </div>
                    }


                </div>
            </div>
        </div>
    </div>

</div>
</div>


@{
	ViewBag.Title = "Photos";
}
<div class="middle-sidebar-bottom">
    <div class="middle-sidebar-left pe-0">
        <div class="row">
            <div class="col-xl-12">


                <div class="row ps-2 pe-2">

                    @foreach (Post post in (List<Post>)ViewBag.Photos)
                    {
                        <div class="card-body d-flex pt-4 ps-4 pe-4 pb-0 border-top-xs bor-0">
                            <figure class="avatar me-3"><img src="@post.Imj" alt="image" class="shadow-sm rounded-circle w45"></figure>
                        </div>
                    }


                </div>
            </div>
        </div>
    </div>

</div>
